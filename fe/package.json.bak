    "native",
    "react-native",
    "starter-kit",
    "webpack"
  ],
  "scripts": {
    "android-setup-port": "adb reverse tcp:8081 tcp:8080",
    "bundle": "rnws bundle -I ./src/ios/main.ios -A ./src/android/main.android --iosBundlePath ./target/classes/META-INF/resources/webjars/fe/0.0.1-SNAPSHOT/main.ios.bundle.js --androidBundlePath ./target/classes/META-INF/resources/webjars/fe/0.0.1-SNAPSHOT/main.android.bundle.js",
    "test": "snyk test && npm run lint && npm run snyk-protect",
    "lint": "eslint src",
    "start": "rnws start",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jhabdas/react-native-webpack-starter-kit"
  },
  "engines": {
    "node": ">=4 <7",
    "npm": ">=2 <4"
  },
  "dependencies": {
    "babel-preset-react": "^6.24.1",
    "react-native": "^0.21.0",
    "snyk": "^1.69.6"
  },
  "devDependencies": {
    "babel-core": "^6.7.2",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react-native": "^1.5.2",
    "babel-preset-stage-1": "^6.5.0",
    "eslint": "^2.2.0",
